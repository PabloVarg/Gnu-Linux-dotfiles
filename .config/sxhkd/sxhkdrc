super + Return
    $TERMINAL

super + shift + Return
    $TERMINAL -c floatingSet

super + i
    $TERMINAL -e $RESOURCES

super + e
    $TERMINAL -e $FILE

super + p
    dmenu_run -nf '#FFFFFF' -nb '#292826' -sb '#8C8C8C' -sf '#292826' -b -i

super + n
    $BROWSER

super + shift + n
    $BROWSER --incognito

meta  + y
    feh --randomize --bg-fill $HOME/Wallpapers/*

meta  + l
    $TERMINAL -e 'betterlockscreen -u ~/Wallpapers/'

meta  + s
    pavucontrol

meta  + w
    $TERMINAL -e nmtui

super + x
    betterlockscreen -t $(whoami) -l

super + shift + x
    betterlockscreen -t $(whoami) -s

super + Print
    take_screen

XF86Calculator
    $TERMINAL -c floatingSet -e python

XF86AudioMute
    amixer sset 'Master' toggle

XF86AudioLowerVolume
    amixer sset 'Master' 1%-

XF86AudioRaiseVolume
    amixer sset 'Master' 1%+

XF86AudioPlay
    playerctl play-pause

XF86AudioPause
    playerctl play-pause

XF86AudioNext
    playerctl next

XF86AudioPrev
    playerctl previous
